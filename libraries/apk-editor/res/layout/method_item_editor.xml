<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  >
  
  <LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  >
  <TextView
    android:id="@+id/access_flags_view"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="访问修饰符:"
    android:textSize="15dip" />
<EditText
    android:id="@+id/access_flags_edit"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:textSize="17dip" />

</LinearLayout>

<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  >
  <TextView
    android:id="@+id/method_name_view"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="方法名:"
    android:textSize="15dip" />
<EditText
    android:id="@+id/method_name_edit"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:textSize="17dip" />

</LinearLayout>

<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  >
  <TextView
    android:id="@+id/method_descriptor_view"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="描述: "
    android:textSize="15dip" />
<EditText
    android:id="@+id/method_descriptor_edit"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:textSize="17dip" />

</LinearLayout>

<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  >
  <TextView
    android:id="@+id/register_count_view"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="寄存器数量: "
    android:textSize="15dip" />
<EditText
    android:id="@+id/register_count_edit"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:textSize="17dip" />

</LinearLayout>



</LinearLayout>
